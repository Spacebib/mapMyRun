{
    "name": "spacebib/map-my-run",
    "description": "Integrate with Under Armour MapMyRun API",
    "type": "libray",
    "license": "MIT",
    "minimum-stability": "dev",
    "require": {
        "ext-json": "*",
        "spacebib/oauth-underarmour": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "Spacebib\\MapMyRun\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Spacebib\\MapMyRun\\Tests\\": "tests/src/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^5.0",
        "squizlabs/php_codesniffer": "~2.0"
    }
}
